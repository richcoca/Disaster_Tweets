    "def data_cleaning(text):\n",
    "    text = text.lower()\n",
    "    # Format regular expressions for data cleaning\n",
    "    text = re.sub(r'[_\"\\-;%()|+&=*%.,!?:#$@\\[\\]/]', ' ', text)\n",
    "    \n",
    "    # remove stopwords\n",
    "    text = text.split()\n",
    "    stop = set(stopwords.words(\"english\"))\n",
    "    text = [w for w in text if not w in stop]\n",
    "    text = \" \".join(text)\n",
    "\n",
    "\n",
    "    return text"
   ]
  },
